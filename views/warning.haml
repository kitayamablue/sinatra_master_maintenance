%h2 警告メッセージ
%div{:class => "containar"}
	%table{:border => 1, :class => "table table-striped table-hover table-bordered table-condensed"}
		%thead
			%tr
				%th 警告ID
				%th 警告メッセージ
		%tbody
			- @warnings.each do |warning|
				%tr
					%td= warning.Warning
					%td= warning.WarningMsg
:erb
		<%= will_paginate @warnings , :renderer => PaginateRenderer %>